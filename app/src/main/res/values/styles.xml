<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/theme_color_2</item>
        <item name="colorPrimaryDark">@color/theme_color</item>
        <item name="colorAccent">@color/theme_color</item>
    </style>
    <style name="AppTheme.ActionBar.Transparent" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@android:color/transparent</item>

    </style>
</resources>
